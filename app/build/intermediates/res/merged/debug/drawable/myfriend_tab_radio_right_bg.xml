<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- tab  的背景选择器 -->
    <!-- 选中状态 -->
    <item android:state_checked="true"><shape android:shape="rectangle">
            <solid android:color="#01D19E" />

            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp" />
        </shape></item>
    <!-- 未选中状态 -->
    <item android:state_checked="false"><shape android:shape="rectangle">
            <solid android:color="#fff" />

            <corners android:bottomRightRadius="5dp" android:topRightRadius="5dp" />
        </shape></item>

</selector>